PREFIX rso: <https://www.blinded-for-review/rso#>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
PREFIX qudt: <http://qudt.org/schema/qudt/>
SELECT ?indicator ?value ?number ?unit  WHERE {
     rso:Organization1 rso:publish ?r .
     ?r a rso:Report ;
        rdfs:label ?report ;
        rso:hasReportValue ?v .
     ?v a rso:ReportValue ;
        rdfs:label ?value ;
        rso:hasNumericalValue ?number ;
        qudt:hasUnit ?u ;
        rso:reportsOnIndicator rso:Indicator94_E1-7-53-b .
    rso:Indicator94_E1-7-53-b rdfs:label ?indicator . 
    ?u rdfs:label ?unit .
    FILTER(LANGMATCHES(LANG(?unit), "en"))
}
